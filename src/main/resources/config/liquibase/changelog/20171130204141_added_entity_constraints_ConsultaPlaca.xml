<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ConsultaPlaca.
    -->
    <changeSet id="20171130204141-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="municipio_id"
                                 baseTableName="consulta_placa"
                                 constraintName="fk_consulta_placa_municipio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="municipio"/>

        <addForeignKeyConstraint baseColumnNames="corporacion_id"
                                 baseTableName="consulta_placa"
                                 constraintName="fk_consulta_placa_corporacion_id"
                                 referencedColumnNames="id"
                                 referencedTableName="corporacion"/>

    </changeSet>
</databaseChangeLog>
